{
    "configurations": [
        {
            "name": "PlatformIO",
            "includePath": [
                "${workspaceFolder}/Modules/**",
                "~/Documents/Arduino/**",
                "~/Documents/Arduino/Libraries/**",
                "~/Documents/Arduino/Hardware/**",
                "~/Applications/Arduino.app/**",
                "~/Documents/Arduino/hardware/MegaCore-2.1.3/avr/cores/**",
                "~/Documents/Arduino/hardware/MegaCore-2.1.3/avr/libraries/**",
                "${workspaceFolder}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/Modules/**",
                    "~/Documents/Arduino/**",
                    "~/Documents/Arduino/Libraries/**",
                    "~/Documents/Arduino/Hardware/**",
                    "~/Applications/Arduino.app/**",
                    "~/Documents/Arduino/hardware/MegaCore-2.1.3/avr/cores/**",
                    "~/Documents/Arduino/hardware/MegaCore-2.1.3/avr/libraries/**",
					"${workspaceFolder}"
                ]
            },
            "defines": [
				"_B107AA_=true"
            ],
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "~/.platformio/packages/toolchain-atmelavr/bin/avr-gcc",
            "compilerArgs": [
                "-mmcu=atmega2560"
            ]
        }
    ],
    "version": 4
}